{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "include": ["src/**/*"],
  "exclude": ["Example", "ExampleExpo", "scripts", "docusaurus", "docs"],
  "compilerOptions": {
    "types": ["react-native"],
    "baseUrl": ".",
    "paths": {
      "@sliders/*": ["src/components/Sliders/*"],
      "@panels/*": ["src/components/Panels/*"],
      "@assets/*": ["src/assets/*"],
      "@thumb": ["src/components/Thumb/Thumb"],
      "@colorKit": ["src/colorKit/colorKit"],
      "@context": ["src/AppContext"],
      "@styles": ["src/styles"],
      "@utils": ["src/utils"],
      "@types": ["src/types"]
    },
    "noEmit": false,
    "declaration": false,
    "module": "ESNext",
    "target": "ESNext",
    "isolatedModules": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "importsNotUsedAsValues": "error"
  }
}
