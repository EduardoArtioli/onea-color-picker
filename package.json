{
  "name": "reanimated-color-picker",
  "version": "0.0.32-beta",
  "description": "Pure JavaScript color picker for react-native",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "remove-web": "@powershell rm src -r -Include *.web.js",
    "build": "npm run remove-web && babel src -d src --out-file-extension .web.js --ignore src/index.js",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run remove-web"
  },
  "keywords": [
    "react-native",
    "reanimated",
    "color",
    "picker",
    "palette"
  ],
  "files": [
    "src"
  ],
  "repository": "https://github.com/alabsi91/reanimated-color-picker",
  "author": "Ahmed Alabsi <alabsi91@gmail.com> (https://github.com/alabsi91)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alabsi91/reanimated-color-picker/issues"
  },
  "homepage": "https://github.com/alabsi91/reanimated-color-picker",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.12.9",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-native-reanimated": "^2.9.1"
  },
  "peerDependencies": {
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-reanimated": ">=2.0.0",
    "expo": ">=44.0.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  }
}
